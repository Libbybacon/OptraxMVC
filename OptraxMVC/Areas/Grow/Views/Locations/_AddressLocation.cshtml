@model AddressLocation

@{
    bool showEdit = ViewBag.ShowEdit as bool? ?? false;
    string prefix = ViewBag.Prefix;
}
<input type="hidden" id="addy-string" value="@Model.Address?.AddressString" />

<partial name="_Details" model="Model" />
<partial name="_Edit" model="Model" />

@await Component.InvokeAsync("Address", new { address = Model.Address, showEdit = showEdit, prefix = prefix })