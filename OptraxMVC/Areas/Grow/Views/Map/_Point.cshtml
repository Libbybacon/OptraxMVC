@using OptraxDAL.Models.Maps
@using OptraxMVC.Models

@model MapPoint

@{
    FormVM formVM = ViewBag.FormVM;
}

<form asp-action="@formVM.Action" asp-controller="Map" method="post" id="mapObjForm" data-id="@Model.Id" data-obj="Point">
    @Html.AntiForgeryToken()

    <input type="hidden" asp-for="IconId" />
    <input type="hidden" asp-for="Latitude" />
    <input type="hidden" asp-for="Longitude" />
    <partial name="_MapObject.cshtml" model="Model" />
</form>



